size: 2rem;\n  transition: ease 0.5s;\n  -webkit-transition: ease 0.5s;\n  padding: 10px 20px;\n  margin: 0;\n  outline: none;\n}\n\n#start-button button:hover {\n  border: solid 1px #333;\n  cursor: pointer;\n}\n\n\n.star {\n  position: absolute;\n  width: 0.5px;\n  height: 0.5px;\n  background-color: white;\n  animation: fade 10s;\n}\n\n@keyframes fade {\n  0% {\n    opacity: 0;\n  }\n\n  100% {\n    opacity: 1;\n  }\n}\n\n.intro {\n  position: absolute;\n  animation: intro 6s ease-out 1s;\n  color: rgb(75, 213, 238);\n  font-weight: 400;\n  font-size: 2rem;\n  opacity: 0;\n  top: 50%;\n  left: 50%;\n  margin: auto;\n  transform: translate(-50%);\n}\n\n.logo {\n  position: absolute;\n  top: 20%;\n  z-index: 1;\n  margin: auto;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  width: 100%;\n  animation: logo 9s ease-out 9s;\n  opacity: 0;\n  font-family: StarWars;\n  text-align: center;\n  font-size: 10rem;\n}\n\n@keyframes logo {\n  0% {\n    transform: scale(2.75);\n    opacity: 1;\n  }\n  50% {\n    opacity: 1;\n  }\n  100% {\n    opacity: 0;\n    transform: scale(0.1);\n  }\n}\n\n@keyframes intro {\n  0% {\n    opacity: 0;\n  }\n  20% {\n    opacity: 1;\n  }\n  90% {\n    opacity: 1;\n  }\n  99% {\n    opacity: 0;\n  }\n  100% {\n    display: none;\n  }\n}\n\n\np, .logo {\n  color: #FFFF82;\n}\n\n#board {\n  font-family: Century Gothic, CenturyGothic, AppleGothic, sans-serif;\n  transform: perspective(300px) rotateX(15deg);\n  transform-origin: 50% 100%;\n  text-align: justify;\n  position: absolute;\n  font-weight: bold;\n  overflow: hidden;\n  font-size: 350%;\n  height: 50em;\n  width: 18em;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  margin: 0 auto;\n  display: flex;\n  justify-content: center;\n}\n\n#board:after {\n  position: absolute;\n  content: ' ';\n  bottom: 60%;\n  left: 0;\n  right: 0;\n  top: 0;\n}\n\n#content {\n  animation: scroll 100s linear 16s;\n  position: absolute;\n  top: 100%;\n}\n\n#title {\n  font-weight: 500;\n}\n\n#title, #subtitle {\n  text-align: center;\n}\n\n@keyframes scroll {\n  0% {\n    top: 100%;\n  }\n  100% {\n    top: -170%;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"a651b3a17585d3a0953c2198d53e12c3","cacheData":{"env":{}}}